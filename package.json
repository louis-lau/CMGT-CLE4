{
  "name": "phaser-workshop",
  "version": "1.0.0",
  "description": "Phaser 3 Workshop with Typescript and Parcel",
  "main": "src/index.html",
  "scripts": {
    "build": "parcel build src/index.html --public-url ./ --out-dir docs --no-source-maps",
    "watch": "parcel watch src/index.html --out-dir docs",
    "start": "parcel serve src/index.html --open --out-dir dev-docs",
    "clean": "rm -v docs/* dev-docs/*"
  },
  "keywords": [
    "phaser3"
  ],
  "author": "HR-CMGT",
  "license": "ISC",
  "dependencies": {
    "@types/node": "^11.15.18",
    "@types/webpack-env": "^1.15.2",
    "colors.css": "^3.0.0",
    "phaser": "^3.24.1"
  },
  "devDependencies": {
    "cssnano": "^4.1.10",
    "parcel-bundler": "^1.12.4",
    "typescript": "^3.9.6"
  }
}
